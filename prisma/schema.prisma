generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model shared_sessions {
  short_url    String    @id @db.VarChar(10)
  session_data Json
  created_at   DateTime? @default(now()) @db.Timestamp(6)
  hashcheck    String    @db.VarChar(64) @unique
}
